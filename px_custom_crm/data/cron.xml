<odoo>
    <data noupdate="1">

        <record id="ir_cron_move_fresh_leads_midnight" model="ir.cron">
            <field name="name">Move Fresh Leads to Not Communicated at Midnight</field>
            <field name="model_id" ref="crm.model_crm_lead"/>
            <field name="state">code</field>
            <field name="code">model._cron_move_fresh_leads()</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
<!--            <field name="numbercall">-1</field>-->
<!--            <field name="doall">False</field>-->
            <field name="nextcall" eval="(DateTime.now() + relativedelta(minutes=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

        <record id="ir_cron_move_to_rotated_stage" model="ir.cron">
            <field name="name">Move Leads to Rotated Stage if Salesperson Changed</field>
            <field name="model_id" ref="crm.model_crm_lead"/>
            <field name="state">code</field>
            <field name="code">model._cron_move_to_rotated_stage()</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
<!--            <field name="numbercall">-1</field>-->
<!--            <field name="doall">False</field>-->
            <field name="nextcall" eval="(DateTime.now() + relativedelta(minutes=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

    </data>
</odoo>
